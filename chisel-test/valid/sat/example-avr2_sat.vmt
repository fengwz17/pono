(declare-const u (_ BitVec 2))
(declare-const v (_ BitVec 2))
(declare-const u.next (_ BitVec 2))
(declare-const v.next (_ BitVec 2))
(define-fun sv1 () (_ BitVec 2) (! u :next u.next))
(define-fun sv2 () (_ BitVec 2) (! v :next v.next))

(define-fun init () Bool
(! (and (= u #b01) (= v #b01)) :init true))

(define-fun trans () Bool
(! (and (= u.next (bvadd u v)) (= v.next (bvadd v #b01))) :trans true))

(define-fun prop () Bool
(! (not (= (bvadd u v) #b01)) :invar-property 0))  
