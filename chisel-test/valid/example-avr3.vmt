(declare-const u (_ BitVec 3))
(declare-const v (_ BitVec 3))
(declare-const u.next (_ BitVec 3))
(declare-const v.next (_ BitVec 3))
(define-fun sv1 () (_ BitVec 3) (! u :next u.next))
(define-fun sv2 () (_ BitVec 3) (! v :next v.next))

(define-fun init () Bool
(! (and (= u #b001) (= v #b001)) :init true))

(define-fun trans () Bool
(! (and (= u.next (bvadd u v)) (= v.next (bvadd v #b001))) :trans true))

(define-fun prop () Bool
(! (not (= (bvadd u v) #b001)) :invar-property 0))  
